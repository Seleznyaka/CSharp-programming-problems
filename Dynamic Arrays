/*
    Динамический массив
    List<T>
*/
using System.Collections.Generic;
// T - тип данных

List<int> numbers = new List<int>() { 1 };

numbers.Add(5); // Добавление в конец списка
numbers.Remove(2);
numbers.Insert(1, 10);

for (int i = 0; i < numbers.Count; i++)
    Console.WriteLine(numbers[i]);

